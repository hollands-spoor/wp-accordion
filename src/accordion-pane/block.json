{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "hs-blocks/accordion-pane",
	"version": "1.0.2",
	"title": "Accordion Pane",
	"category": "widgets",
	"icon": "",
	"description": "Accordion Pane Block for Accordion.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
        "heading": {
            "type": "string",
            "default": ""
        },
