			"type": "object",
			"default": {
				"headerTag" : "h3",
				"iconPosition" : "right",
				"iconType" : "plusminus"
			}
		}
    },
	"usesContext": [
		"hs-blocks/accordion"
	],
	"textdomain": "accordion",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
